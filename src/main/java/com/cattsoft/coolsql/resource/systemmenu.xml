<?xml version="1.0" encoding="utf-8"?>
<!--
all children element:
	1. menu
-->
<menus>
<!--
          menuProperties:
              1.label
              2.disabled
              3.location
              4.loadlistener
              5.icon
          
          menuItemProperties:
          	  1.label
          	  2.type(plain,check,radio)
          	  3.loadlistener
          	  4.icon
          menuItem subElement:
              1.tooltip
              2.action Class
              3.shortcut
              4.enableCheck
              5.icon  ->
              			property:
              			        1.type(file,resource)  //file:本地文件，resource:资源绑定
               
 -->
